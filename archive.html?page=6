		document.oncontextmenu = function() {
		    window.event.returnValue = false;
		};
		document.onkeydown=function (e) {
	        var currKey=0,evt=e||window.event;
	        currKey=evt.keyCode||evt.which||evt.charCode;
	        if (currKey == 123) {
	            window.event.returnValue = false;
	        }
	    };
